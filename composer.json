{
  "name": "wsr/myleaflet",
  "description": "Openstreetmap / leaflet maps with radial search and hierarchic categories.",
  "keywords": ["TYPO3", "Openstreet map", "extension", "radial search"],
  "homepage": "https://www.web-services-ruhs.de",
  "authors": [
    {
      "name": "Joachim Ruhs",
      "email": "postmaster@joachim-ruhs.de"
    }
  ],
  "license": "MIT",
  "type": "typo3-cms-extension",
  "require": {
	"typo3/cms-core": "^12.4 || ^13",
	"typo3/cms-frontend": "^12.4 || ^13"
  },
  "replace": {
    "typo3-ter/myleaflet": "self.version"
  },
  "autoload": {
    "psr-4": {
      "WSR\\Myleaflet\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
       "web-dir": ".Build/public",
       "extension-key": "myleaflet"
    }
  }
}
